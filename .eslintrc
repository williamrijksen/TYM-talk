{
  "root": true,
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "strict": [2, "global"],
    "one-var": [2, "always"],
    "new-cap": [2, {"capIsNewExceptions": ["L"]}]
  },
  "parser": "babel-eslint",
  "globals": {
    "jQuery": false,
    "$": true,
    "OS_IOS": true,
    "OS_ANDROID": true,
    "_": true,
    "Backbone": true,
    "Alloy": true,
    "require": true,
    "module": true,
    "exports": true,
    "Ti": true,
    "arguments": true,
    "WPATH": true,
    "L": true,
    "__parentSymbol": true
  }
}
